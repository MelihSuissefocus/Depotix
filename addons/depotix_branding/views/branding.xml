<?xml version="1.0" encoding="utf-8"?>
<depotix>
    <!-- Branding-Modul für Depotix -->
    
    <!-- CSS Asset Bundle um Module zu verstecken -->
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/depotix_branding/static/src/css/hide_modules.css"/>
        </xpath>
    </template>
    
    <!-- Titel-Anpassungen können über Company-Einstellungen oder JavaScript erfolgen -->
</depotix>
